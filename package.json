{
  "name": "fluentui-svelte",
  "private": true,
  "scripts": {
    "build": "turbo build --filter docs",
    "build:all": "turbo build",
    "dev": "turbo dev --filter docs",
    "dev:all": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md,svelte}\""
  },
  "devDependencies": {
    "prettier": "^3.1.0",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=18"
  },
  "pnpm": {
    "overrides": {
      "svelte": "^5.0.0-next.1"
    }
  },
  "packageManager": "pnpm@8.11.0"
}